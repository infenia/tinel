[run]
source = tinel
branch = True
parallel = True
concurrency = multiprocessing
data_file = .coverage

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    @abstractmethod
precision = 2
show_missing = True
skip_covered = False
sort = Cover

[html]
directory = htmlcov
title = Tinel Coverage Report

[xml]
output = coverage.xml
